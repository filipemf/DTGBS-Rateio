{
  "name": "rateio",
  "productName": "DTGBS - Rateio",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "package:build": "npm run make && electron-builder --publish=always",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "publish2": "electron-builder --publish=always -mwl",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Fiipe Ferreira",
    "email": "filipemarquesf.dev@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.3.0",
    "jquery": "^3.7.0",
    "node-xlsx": "^0.21.2",
    "sweetalert2": "^11.7.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@electron-forge/publisher-github": "^6.1.1",
    "electron": "24.4.0"
  },
  "publish": [
    {
      "provider": "github",
      "owner": "filipemf",
      "repo": "DTGBS-Rateio"
    }
  ],
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./resources/icon2.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip"
        }
      ],
      "publishers": [
            {
              "name": "@electron-forge/publisher-github",
              "config": {
                "repository": {
                  "owner": "filipemf",
                  "name": "DTGBS-Rateio"
                }
              }
            }
          ]
        
      
    }
  }
}
